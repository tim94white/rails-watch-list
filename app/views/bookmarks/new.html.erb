<div class="banner">
  <h1>New Bookmark</h1>
</div>

<div class="container">
  <div class="offset-sm-3 col-sm-6">
    <%= simple_form_for [@list, @bookmark] do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit %>
    <% end %>
  </div>
</div>

<div>
  <%= link_to "Go back", lists_path %>
</div>